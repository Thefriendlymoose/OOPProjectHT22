<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.stage.Stage?>

<Stage xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
    <scene>
        <Scene>
            <AnchorPane stylesheets="@../stylesheet.css" prefHeight="200" prefWidth="200">
            <children>
               <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="200.0">
                  <children>
                     <Label text="Open">
                        <font>
                           <Font size="24.0" />
                        </font>
                        <VBox.margin>
                           <Insets top="25.0" />
                        </VBox.margin>
                     </Label>
                     <TextField fx:id="searchField">
                        <VBox.margin>
                           <Insets bottom="25.0" left="20.0" right="20.0" top="25.0" />
                        </VBox.margin>
                     </TextField>
                     <Button styleClass="generic-button" fx:id="openButton" mnemonicParsing="false" text="Open" />
                  </children>
               </VBox>
            </children></AnchorPane>
        </Scene>
    </scene>
</Stage>
