<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.TextField?>
<AnchorPane prefHeight="76.0" prefWidth="348.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.customerControllers.ContactEditCardController">
   <children>
      <TextField fx:id="name" editable="false" layoutY="27.0" prefHeight="13.0" prefWidth="158.0" text="ContactName">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="number" editable="false" layoutY="7.0" prefHeight="20.0" prefWidth="152.0" text="ContactNumber">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="email" editable="false" layoutY="49.0" prefHeight="19.0" prefWidth="152.0" text="ContactEmail">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Button onAction="#editContactHandler" layoutX="166.0" layoutY="10.0" mnemonicParsing="false" prefHeight="54.0" prefWidth="80.0" text="Edit" />
      <Button fx:id="cancelButton" onAction="#cancelHandler" visible="false" layoutX="258.0" layoutY="11.0" mnemonicParsing="false" prefHeight="54.0" prefWidth="80.0" text="Cancel" />
   </children>
</AnchorPane>
