<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.stage.Stage?>


<Stage xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.customercontrollers.CustomerSearchController">
   <scene>
      <Scene>
         <root>
            <AnchorPane prefHeight="290.0" prefWidth="454.0">
               <children>
                  <VBox alignment="CENTER" layoutX="130.0" layoutY="111.0" prefHeight="39.0" prefWidth="195.0" spacing="15.0">
                     <children>
                        <TextField fx:id="idtextField" text="Enter Customer ID" />
                        <Button onAction="#openCustomerHandler" mnemonicParsing="false" text="Open" />
                     </children>
                  </VBox>
               </children>
            </AnchorPane>
         </root>
      </Scene>
   </scene>
</Stage>
