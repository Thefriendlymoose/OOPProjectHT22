<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.stage.Stage?>

<Stage xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.customercontrollers.ContactEditorController">
   <scene>
      <Scene>
         <root>
            <AnchorPane prefHeight="461.0" prefWidth="535.0">
               <children>
                  <ScrollPane layoutX="31.0" layoutY="14.0" prefHeight="374.0" prefWidth="365.0">
                     <content>
                        <VBox fx:id="contactBox" prefHeight="328.0" prefWidth="215.0" />
                     </content>
                  </ScrollPane>
                  <Button layoutX="419.0" layoutY="184.0" mnemonicParsing="false" onAction="#addContactHandler" prefHeight="94.0" prefWidth="95.0" text="Add Contact" />
                  <Button layoutX="31.0" layoutY="411.0" mnemonicParsing="false" onAction="#closeHandler" prefHeight="27.0" prefWidth="81.0" text="Close" />
               </children>
            </AnchorPane>
         </root>
      </Scene>
   </scene>
</Stage>
