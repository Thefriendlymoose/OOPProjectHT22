<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.stage.Stage?>


<Stage xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.customerControllers.ContactEditorController">
   <scene>
      <Scene>
         <root>
            <AnchorPane prefHeight="461.0" prefWidth="398.0">
               <children>
                  <ScrollPane layoutX="31.0" layoutY="14.0" prefHeight="374.0" prefWidth="255.0">
                     <content>
                        <VBox fx:id="contactBox" prefHeight="328.0" prefWidth="215.0" />
                     </content>
                  </ScrollPane>
                  <Button onAction="#addContactHandler" layoutX="295.0" layoutY="181.0" mnemonicParsing="false" prefHeight="94.0" prefWidth="95.0" text="Add Contact" />
                  <Button onAction="#saveHandler" layoutX="31.0" layoutY="411.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="81.0" text="Save" />
                  <Button onAction="#cancelHandler" layoutX="130.0" layoutY="411.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="85.0" text="Cancel" />
               </children>
            </AnchorPane>
         </root>
      </Scene>
   </scene>
</Stage>
