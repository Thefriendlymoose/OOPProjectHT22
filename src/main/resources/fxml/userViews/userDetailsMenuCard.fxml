                <Label fx:id="cardUserIDLabel" styleClass="generic-form-label" text="UserID: " />
                <Label fx:id="cardNameLabel" styleClass="generic-form-label" text="Name: " />
                <Label fx:id="cardPremissionLabel" styleClass="generic-form-label" text="Premissions: " wrapText="true" />
                <HBox>
                    <Button fx:id="editUserButton" alignment="BASELINE_LEFT" mnemonicParsing="false" styleClass="generic-button" text="Edit User" />
                    <Region HBox.hgrow="ALWAYS" />
                    <Button fx:id="removeUserButton" alignment="BASELINE_RIGHT" mnemonicParsing="false" styleClass="generic-button , form-cancel-button" text="Remove User" />
                </HBox>
            </children>
        </VBox>
    </children>
